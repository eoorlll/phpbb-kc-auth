{
  "name": "tc/keycloak",
  "type": "phpbb-extension",
  "description": "Keycloak auth provider for phpBB",
  "version": "0.1.0",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Teracloud",
      "email": "evgenii.podrezenko@vmssoftware.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "jumbojett/openid-connect-php": "v1.0.2"
  },
  "autoload": {
    "psr-4": {
      "tc\\keycloak\\": ""
    }
  },
  "extra": {
    "display-name": "Keycloak Auth",
    "soft-require": {
      "phpbb/phpbb": ">=3.2.0,<4.0.0@dev"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
